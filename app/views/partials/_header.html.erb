      <div class="navbar-brand">
        <%= link_to "23andMe", root_path %>
      </div>
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to "Home", root_path %></li>
          <% if current_token %>
            
            <li><%= "Welcome, email address" %></li>
            <li><%= link_to "Logout", logout_path, method: :delete %></li>
