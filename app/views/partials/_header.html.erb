            <li><%= link_to "Research", root_path %></li>
            <li><%= link_to "Buy", root_path %></li>
            <li><%= link_to "Sign in with 23andMe", "https://api.23andme.com/authorize/?redirect_uri=http://localhost:3000/receive_code&response_type=code&client_id=fa14c16e8d247c33723730c697802dd9&scope=basic%20ancestry%20email%20haplogroups" %></li>
          <% end %>
